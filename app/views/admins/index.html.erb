<div class="container">
<h1>Admins Table</h1>
<p id='notice'><%= notice%></p>
<table class="table table-striped">
<tr>
<td>Firstname</td>
<td>Lastname</td>
<td>Age</td>
<td>Person_dob</td>
<th colspan='3'></th>
</tr>
<% @admins.each do |admin|%>
    <tr>
<td><%=admin.firstname%></td>
<td><%=admin.lastname%></td>
<td><%=admin.age%></td>
<td><%=admin.person_dob%></td>
<td>
<%= link_to'Show',admin %></td>
<td><%= link_to'Edit',edit_admin_path(admin) %></td>
<td><%= link_to'Destroy',admin, method: :delete,data:{confirm:'Are you sure?'} %></td>
</tr>
<% end %>
<%= link_to'New Admin',new_admin_path %>
</table>
 <%=link_to("go to Home",{:controller=>'manage',:action=>"home"})  %>
</div>
